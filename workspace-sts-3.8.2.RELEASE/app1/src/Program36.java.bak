class Program36{
	public static void main(String[] args){
		int divident = 145;
		int divisor = 4;
		int quotient = 0;
		while(divident>=divisor){
			divident = divident-divisor;
			quotient++;
		}
		System.out.println("Quotient is:"+quotient);
		System.out.println("Remainder is:"+divident);
	}
}
