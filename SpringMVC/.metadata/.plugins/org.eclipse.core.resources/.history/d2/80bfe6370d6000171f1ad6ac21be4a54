var STUDENT = {
		
	createFunction : function(){
		var studentRegistration = {};
		studentRegistration.firstName = $("#first_name").val();
		studentRegistration.lastName = $("#last_name").val();
		studentRegistration.fullName = $("#full_name").val();
		studentRegistration.email = $("#password").val();
		studentRegistration.password = $("#password").val();
		studentRegistration.gender = $("#password").val();
		studentRegistration.country = $("#password").val();
		studentRegistration.state = $("#password").val();
		studentRegistration.city = $("#password").val();
		studentRegistration.address = $("#password").val();
		studentRegistration.zipcode = $("#password").val();
		studentRegistration.contactno = $("#password").val();
		console.log(JSON.stringify(studentRegistration));
		
		$.ajax({
			url :"/StudentRegistration/studentReg",
			type :"POST",
			data :{"studentRegistration":JSON.stringify(studentRegistration)},
			dataType :'json',
			success :function(response){
				alert(response);
				var str = JSON.stringify(response);
				alert(str);
				console.log(str);
			},
			error :function(e){
				console.log(e);
				alert(e);
	       }
		});
		
 	},	
 	
};